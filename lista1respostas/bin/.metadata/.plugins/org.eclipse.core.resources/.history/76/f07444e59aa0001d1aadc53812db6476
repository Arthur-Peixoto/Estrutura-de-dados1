package lista1respostas.exerciciosdeentradasaídaoperadores;

public class Main5respota {
    public static void Main(String Args[]){
        double double1 = 10.7;
        double double2 = 23.5; 
        double double3 = 45.3;

        System.out.println("Média = "+ ((double1 + double2 + double3)/3));
    }
}
